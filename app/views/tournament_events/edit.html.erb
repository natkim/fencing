<h1>Editing Tournament Event #<%= @tournament_event.id %></h1>

<form action="/tournament_events/<%= @tournament_event.id %>/update">
  <div>
    <label for="tournament_id">tournament_id</label><br />
    <input id="tournament_id" name="tournament_id" value="<%= @tournament_event.tournament_id  %>" type="text" />
  </div>
  <div>
    <label for="event_id">event_id</label><br />
    <input id="event_id" name="event_id" value="<%= @tournament_event.event_id  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


